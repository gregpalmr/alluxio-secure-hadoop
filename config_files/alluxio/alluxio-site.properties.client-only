# FILE: alluxio-site.properties file for clients (hiveserver2)
#

alluxio.master.hostname=alluxio
alluxio.security.authentication.type=KERBEROS
alluxio.security.kerberos.unified.instance.name=alluxio.docker.com
alluxio.security.kerberos.client.principal=hive/hadoop.docker.com@EXAMPLE.COM
alluxio.security.kerberos.client.keytab.file=/etc/security/keytabs/hive.service.keytab
alluxio.user.file.writetype.default=ASYNC_THROUGH
alluxio.user.file.readtype.default=CACHE

# end of file
